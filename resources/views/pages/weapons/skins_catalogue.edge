@layout.app()
  @body({
    class:['bg-default']
  })
    @eval(await auth.check())
    @!nav()
    <div class="grid grid-cols-3 gap-4 p-28">
      @each(skin in data)
        <a href="{{ route('skin', [skin.uuid]) }}">
          @!component('components/weapon/card', { full_render: skin.display_icon, display_name: skin.skin_name })
        </a>
      @end
    </div>
    @!footer()
  @end
@end


