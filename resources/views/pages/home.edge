                      <div class="mx-auto mr-6">
                        <div class="w-48 h-72">
                          <a href="{{ route('skin', [skin.uuid]) }}" class="h-full">
                            @!weapon.card({
                            full_render: skin.displayIcon,
                            display_name: skin.skinName,
                            tier_icon: skin.tierIcon,
                            tier_name: skin.tierName,
                            weapon_name: skin.weaponName,
                            currency,
                            show_case: true
